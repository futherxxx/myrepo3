@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tblkeyboard.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblmonitor.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblmouse.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblpowerpack.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblsystem.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
