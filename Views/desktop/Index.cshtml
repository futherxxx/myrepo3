@model IEnumerable<PCDetailsWeb.Models.tbldesktop>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tblkeyboard.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblmonitor.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblmouse.name)
